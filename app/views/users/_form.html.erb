  <main class="signin-wrapper">
    <div class="tab-content">
   
        
     
      <div class="tab-pane fade in active" id="signin">
        <form id="signinForm" action="index.html" role="form">
          <p class="text-center lead">Editing Profile</p>
          
          <!--div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-envelope-o"></i></span>
              <%# f.label :email %>
             
            
            <%# f.label :displayname %>

            <%# f.text_field :displayname, :class => "form-control" %><br>
    
              
            </div>
          </div><!/.form-group -->

          <div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-globe"></i></span>


          <%= country_select("user", "country") %>

            </div>
          </div><!-- /.form-group -->



          <div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-map"></i></span>
              <%# f.label :email %>
         

            <%= f.label :add_city %> <br>
            <%= f.text_field :city, :autofocus => true, :placeholder => 'Your nearest City', :class => "form-control" %>
    
              
            </div>
          </div><!-- /.form-group -->




          <div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-image"></i></span>

          <%= f.label :add_profile_photo %><br>
    <%= f.file_field :userphoto %><br>

     </div>
          </div><!-- /.form-group -->




          <div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-envelope-o"></i></span>
              <%# f.label :email %>

           <%= f.label :add_profile_cover %><br>
    <%= f.file_field :usercover %><br>


            </div>
          </div><!-- /.form-group -->

          <div class="form-group">
            <div class="input-group input-group-in">
              <span class="input-group-addon"><i class="fa fa-globe"></i></span>
     <%= f.label :intro %><br>
     <%= f.text_area :bio, :autofocus => true, :placeholder => 'Tell us about yourself... in twenty words maximum', :class => "form-control" %>
   </div>
          </div><!-- /.form-group -->

  <br>
 
  
  
  <%= f.submit "Save changes", class: "btn btn-primary" %>
<% end %>
